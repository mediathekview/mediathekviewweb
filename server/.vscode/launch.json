  "version": "0.2.0",
  "configurations": [{
    "type": "node",
    "request": "launch",
    "name": "Launch App without Build",
    "outFiles": ["${workspaceFolder}/dist/**/*.js"],
    "program": "${workspaceFolder}/dist/app.js",
    "sourceMaps": true,
    "showAsyncStacks": true,
    "autoAttachChildProcesses": true,
    "protocol": "inspector"
  },{
    "type": "node",
    "request": "launch",
    "name": "Launch App",
    "preLaunchTask": "build",
    "outFiles": ["${workspaceFolder}/dist/**/*.js"],
    "program": "${workspaceFolder}/dist/app.js",
    "sourceMaps": true,
    "showAsyncStacks": true,
    "autoAttachChildProcesses": true,
    "protocol": "inspector"
  },
  {
    "type": "node",
    "request": "launch",
    "name": "Launch Test",
    "outFiles": ["${workspaceFolder}/dist/**/*.js"],
    "program": "${workspaceFolder}/dist/test.js",
    "sourceMaps": true,
    "showAsyncStacks": true,
    "autoAttachChildProcesses": true,
    "protocol": "inspector"
  }]
}
